/**index.wxss**/
.container{
    width:100%;
    height:100%;
    background: #f5f5f5;
}
.swip{
    width:100%;
}
.home-header {
  width: 100%;
  height: 104rpx;
  box-sizing: border-box;
  padding: 20rpx 36rpx 20rpx 36rpx;
  display: flex;
  background: #fff;
}

.header-search {
  width: 508rpx;
  height: 64rpx;
  background: #f5f5f5;
  border-radius: 100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  padding: 14rpx 35rpx;
  position: relative;
}

.search-left {
  position: absolute;
  top: 23rpx;
  left: 24rpx;
  width: 24rpx;
  height: 24rpx;
}

.search-left image {
  display: block;
  width: 100%;
  height: 100%;
}

.search-right {
  flex: 1;
  height: 100%;
  margin-left: 25rpx;
}

.search-right .search-box {
  height: 100%;
  min-height: 1rem;
  font-size: 26rpx;
  color: #999;
}

.header-address {
  width: 150rpx;
  background: #f5f5f5;
  border-radius: 100rpx;
  margin-left: 20rpx;
  box-sizing: border-box;
  /* padding: 14rpx 30rpx; */
  display: flex;
  justify-content: center;
  align-items: center;
}

.address-icon {
  width: 22rpx;
  height: 27rpx;
}

.address-icon image {
  display: block;
  width: 100%;
  height: 100%;
}

.address-city {
  max-width: 75rpx;
  font-size: 26rpx;
  margin-left: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
